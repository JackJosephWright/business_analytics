library(tidyverse)
library(car)
library(GGally)
library(here)
file<-here('crime-training-data_modified.csv')
df_raw<-read.csv(file)
ggpairs(df_raw, mapping=aes(color=factor(target)))
ggplot(df_raw,aes(x = variable,y = value)) + facet_wrap(~variable) + geom_boxplot()
ggplot(df_raw,aes(x = variable,y = value)) + facet_wrap(~variable) + geom_boxplot()
ggplot(stack(df_raw), aes(x = ind, y = values)) +
geom_boxplot()
ggplot(stack(df_raw), aes(x = as.factor(target), y = values)) +
geom_boxplot()
ggplot(stack(df_raw), aes(x =ind, y = values)) +
geom_boxplot()
ggplot(stack(df_raw), aes(x =ind, y = values)) +
